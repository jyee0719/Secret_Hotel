<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.edu.utar.secret_hotel">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Secret_Hotel">
        <activity android:name=".Profile"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".BookingDate" />
        <activity android:name=".Room6" />
        <activity android:name=".Room5" />
        <activity android:name=".Room4" />
        <activity android:name=".Room3" />
        <activity android:name=".Room2" />
        <activity android:name=".SingleRoom" />
        <activity android:name=".Login" />
        <activity android:name=".Register" />
        <activity android:name=".Service" />
        <activity android:name=".RoomList" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Menu"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>